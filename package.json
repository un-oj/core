{
  "name": "un-oj",
  "type": "module",
  "version": "0.3.0",
  "description": "Unified Online Judge information collector.",
  "author": "UnOJ Team",
  "license": "MIT",
  "homepage": "https://jsr.io/@un-oj/core",
  "repository": {
    "url": "git+https://github.com/un-oj/core.git"
  },
  "keywords": [
    "un-oj",
    "oj",
    "online-judge",
    "competitive-programming"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./platforms/atcoder": {
      "types": "./dist/platforms/atcoder.d.mts",
      "import": "./dist/platforms/atcoder.mjs"
    },
    "./platforms/codeforces": {
      "types": "./dist/platforms/codeforces.d.mts",
      "import": "./dist/platforms/codeforces.mjs"
    },
    "./platforms/hydro": {
      "types": "./dist/platforms/hydro.d.mts",
      "import": "./dist/platforms/hydro.mjs"
    },
    "./platforms/leetcode": {
      "types": "./dist/platforms/leetcode.d.mts",
      "import": "./dist/platforms/leetcode.mjs"
    },
    "./platforms/lyrio": {
      "types": "./dist/platforms/lyrio.d.mts",
      "import": "./dist/platforms/lyrio.mjs"
    },
    "./platforms/luogu": {
      "types": "./dist/platforms/luogu.d.mts",
      "import": "./dist/platforms/luogu.mjs"
    },
    "./platforms/mxoj": {
      "types": "./dist/platforms/mxoj.d.mts",
      "import": "./dist/platforms/mxoj.mjs"
    }
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "obuild",
    "dev": "bun ./scripts/dev.ts",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "bun test --coverage"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "ofetch": "^1.4.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tsconfig/bun": "^1.0.7",
    "@typed-sigterm/eslint-config": "^1.4.1",
    "@types/bun": "^1.2.10",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "obuild": "^0.2.1",
    "typescript": "^5.8.3"
  }
}
